{
  "json.enable": true,
  "python.formatting.provider": "black",
  "python.linting.flake8Enabled": true,
  "languageserver": {
    "dockerfile": {
      "command": "docker-langserver",
      "filetypes": [
        "dockerfile"
      ],
      "args": [
        "--stdio"
      ]
    }
  },
  "Lua.telemetry.enable": false
}
